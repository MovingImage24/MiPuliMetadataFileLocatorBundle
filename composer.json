{
  "name": "mi/puli-metadata-file-locator-bundle",
  "description": "This bundle integrates the mi/puli-metadata-file-locator library with the jms/serializer-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Miehe",
      "email": "alexander.miehe@movingimage.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://bitbucket.org/movingimage24/metadata-puli-file-locator.git"
    }
  ],
  "minimum-stability": "beta",
  "require": {
    "jms/serializer-bundle": "^1.0",
    "mi/metadata-puli-file-locator": "^1.0",
    "php": ">=5.6.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Bundle\\PuliMetadataFileLocatorBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Bundle\\PuliMetadataFileLocatorBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
